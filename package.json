    "@tanstack/react-query": "^5.84.1",
    "@trpc/client": "^11.4.4",
    "@trpc/next": "^11.4.4",
    "@trpc/react-query": "^11.4.4",
    "@trpc/server": "^11.4.4",
    "@types/cookie": "^0.5.4",
    "@types/node": "18.14.0",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@types/ws": "^8.18.1",
    "browserslist": "^4.25.1",
    "classnames": "^2.5.1",
    "cookie": "^0.5.0",
    "dayjs": "^1.11.13",
    "esbuild": "^0.17.19",
    "eslint": "9.32.0",
    "eslint-config-next": "15.4.6",
    "extensible-custom-error": "^0.0.7",
    "immutable": "^4.3.7",
    "next": "15.4.6",
    "next-with-linaria": "^0.5.1",
    "pino": "^8.21.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prisma": "^6.13.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-error-boundary": "^4.1.2",
    "sass": "^1.90.0",
    "scss": "^0.2.4",
    "superjson": "^1.13.3",
    "swr": "^2.3.4",
    "typescript": "^5.9.2",
    "universal-url": "^2.0.0",
    "ws": "^8.18.3",
    "zod": "^3.25.76"
