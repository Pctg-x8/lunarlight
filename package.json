  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@storybook/addon-links": "9.1.1",
    "@storybook/nextjs": "9.1.1",
    "@storybook/react": "9.1.1",
    "@types/jest": "^29.5.14",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.4",
    "eslint-plugin-storybook": "^9.1.1",
    "fast-check": "^3.23.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "storybook": "9.1.1",
    "ts-jest": "^29.4.1",
